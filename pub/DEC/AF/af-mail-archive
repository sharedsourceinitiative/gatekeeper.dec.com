*** 525,536 ****
  	    	else
  	    	    iov[0].iov_base += bytes_read;
  	    	}
! #ifdef EWOULDBLOCK
! 	    else if (errno == EWOULDBLOCK) {
  		_AWaitForReadable(aud);
  		errno = 0;
  	    }
- #endif
  #ifdef SUNSYSV
  	    else if (errno == 0) {
  		_AWaitForReadable(aud);
--- 527,536 ----
  	    	else
  	    	    iov[0].iov_base += bytes_read;
